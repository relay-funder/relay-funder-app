{
  "name": "relayfunder",
  "version": "0.2.14",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "type": "module",
  "scripts": {
    "dev": "pnpm install && prisma generate && NODE_OPTIONS=--max_old_space_size=8192 next dev",
    "build": "NODE_ENV=production next build",
    "build:production": "node prisma/deploy-migrations.js && NODE_ENV=production next build",
    "migrate:deploy": "node prisma/deploy-migrations.js",
    "migrate:check": "prisma migrate status",
    "dev:db": "prisma format && prisma migrate dev",
    "dev:db:seed": "prisma db seed",
    "dev:db:seed:dummy": "SEED_DUMMY_MODE=true prisma db seed",
    "dev:db:clear-and-seed": "node prisma/clear-and-seed.js",
    "start": "next start",
    "lint": "rm -rf .next/cache/eslint/ ; eslint && tsc --noEmit && prettier -c \"./**/*.{ts,tsx,js,jsx,mdx}\" .",
    "test": "vitest run",
    "prepare": "husky",
    "publish": "vercel publish",
    "format:write": "prettier --write \"./**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "chown": "stat -c '%u %g' /app/package.json | awk '{printf \"chown -R %i:%i /app/\", $1,$2}' | bash",
    "postinstall": "prisma generate"
  },
